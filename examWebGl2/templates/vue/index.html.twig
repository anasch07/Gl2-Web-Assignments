{% extends 'base.html.twig' %}

{% block Title %}
    TP : Ajouter un PFE
{% endblock %}

{% block Body %}
    {% for content in app.session.flashBag.get('error') %}
        <div class="alert alert-danger" role="alert">
            {{ content }}
        </div>
    {% endfor %}
    {% for content in app.session.flashBag.get('success') %}
        <div class="alert alert-success" role="alert">
            {{ content }}
        </div>
    {% endfor %}
    {{ form(formView) }}
{% endblock %}

